        <!-- Checkstyle Maven Plugin -->
        <maven-checkstyle-plugin.version>3.1.2</maven-checkstyle-plugin.version>
        <!-- Properties Maven Plugin -->
        <properties-maven-plugin.version>1.1.0</properties-maven-plugin.version>
        <!-- Maven Changelog Plugin -->
        <maven-changelog-plugin.version>2.3</maven-changelog-plugin.version>
        <!-- Changelog start Date -->
        <changelog.start.date>2022-01-01</changelog.start.date>
        <!-- Changelog end date -->
        <changelog.end.date>2023-12-31</changelog.end.date>
        <!-- Changelog date range format -->
        <!-- Format used to specified the changelog start and end date -->
        <changelog.date.range.format>yyyy-MM-dd</changelog.date.range.format>
        <!-- Copy-Rename Maven Plugin -->
        <copy-rename-maven-plugin.version>1.0.1</copy-rename-maven-plugin.version>
        <!-- Maven Resources Plugin -->
        <maven-resources-plugin.version>3.2.0</maven-resources-plugin.version>
        <!-- Appassembler Maven Plugin -->
        <appassembler-maven-plugin.version>2.1.0</appassembler-maven-plugin.version>
        <!-- Exec Maven Plugin -->
        <exec-maven-plugin.version>3.0.0</exec-maven-plugin.version>
        <!-- Failsafe Maven Plugin -->
        <maven-failsafe-plugin.version>3.0.0-M6</maven-failsafe-plugin.version>
        <!-- DataFaker Version -->
        <datafaker.version>1.7.0</datafaker.version>
    </properties>

    <scm>
        <connection>scm:git:https://github.com/TGITS/devtoolbox.git</connection>
        <developerConnection>scm:git:git@github.com:TGITS/devtoolbox.git</developerConnection>
        <tag>HEAD</tag>
    </scm>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>net.datafaker</groupId>
                <artifactId>datafaker</artifactId>
                <version>${datafaker.version}</version>
            </dependency>
            <dependency>
                <groupId>org.projectlombok</groupId>
                <artifactId>lombok</artifactId>
